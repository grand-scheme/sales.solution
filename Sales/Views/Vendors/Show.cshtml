@{
  Layout = "_Layout";
}

@using Sales.Models;

<p>
  [Dummy Text // SHOW VENDORS]
</p>

<ul>
  @foreach (Order order in @Model["orders"])
  {
    <li>
      <a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>
      @order.Name
      </a>
    </li>
  }
</ul>

<a href='/vendors/@Model["vendor"].Id/orders/new'>add new order</a>
<a href='/vendors'>back to all vendors</a>
<a href='/'>back to main</a>